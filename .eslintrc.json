{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "linebreak-style": "off",
        "semi": "off",
        "indent": ["error", 4],
        "no-console": "off",
        "no-await-in-loop": "off",
        "no-param-reassign": "off"
    }
}
